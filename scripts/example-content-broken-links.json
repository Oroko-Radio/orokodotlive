        "format": "",
        "indent": 0,
        "version": 1,
        "children": [
          {
            "mode": "normal",
            "text": "Shows streaming July 22nd, Sept 16th and 11th November. Tune in!",
            "type": "text",
            "style": "",
            "detail": 0,
            "format": 0,
            "version": 1
          }
        ],
        "direction": "ltr",
        "textStyle": "",
        "textFormat": 0
